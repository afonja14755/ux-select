{
  "name": "ux-select",
  "version": "1.0.0",
  "description": "UX Select is a lightweight Native JavaScript plugin that replace native select elements with customization",
  "keywords": [
    "HTMLSelect",
    "select",
    "multiple select",
    "disabled select",
    "group option select",
    "ui select",
    "ux select",
    "lightweight",
    "Native JavaScript"
  ],
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production"
