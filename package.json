{
  "name": "ux-select",
  "version": "1.1.0",
  "description": "UX Select is a lightweight Native JavaScript plugin that replace native select elements with customization",
  "keywords": [
    "HTMLSelect",
    "select",
    "multiple select",
    "disabled select",
    "group option select",
    "ui select",
    "ux select",
    "lightweight",
    "TypeScript",
    "Native JavaScript",
    "Vanilla JavaScript",
    "ES-module",
    "Non ES-module"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "path": "^0.12.7",
    "sass": "^1.64.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/afonja14755/ux-select.git"
  },
  "author": "Afonchenkov Dmitrii <afonja14755@gmail.com>",
  "license": "MIT"
}
